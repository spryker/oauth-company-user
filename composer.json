{
  "name": "spryker/oauth-company-user",
  "type": "library",
  "description": "OauthCompanyUser module",
  "license": "proprietary",
  "require": {
    "league/oauth2-server": "^7.1.0 || ^8.0.0",
    "php": ">=7.2",
    "spryker/company-user": "^2.11.0",
    "spryker/customer": "^7.0.0",
    "spryker/customer-extension": "^1.3.0",
    "spryker/kernel": "^3.30.0",
    "spryker/oauth": "^1.4.0",
    "spryker/oauth-company-user-extension": "^1.1.0",
    "spryker/oauth-extension": "^1.1.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/company": "*",
    "spryker/installer": "*",
    "spryker/oauth-customer-connector": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/installer": "If you want to use Installer plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
